#include <stdio.h>
#include <stdlib.h>
#include <conio.h>

int main(int argc, char *argv[]) {
int a=0,b=0,c=0;
printf("\n Introduce el sueldo del trabajador:");
scanf("%d",&a);
if(a>15000)
{
	b=a*30;
	c=b+a;
	
}
else
   {
	b=a*.15;
	c=b+a;
}
printf("\n El sueldo es: %d",c);

	getch;
}

