#include <stdio.h>
#include <conio.h>

int main(int argc, char *argv[]) {
int cand=0,x=0,morena=0,pri=0,pan=0,prd=0;
for(int x=1;x<=200;x++)
{
	printf("Selecciona el partido a decear: \n ");
	printf("\n *********************************************** \n ");
	printf("\n 1.- morena \n ");
	printf("\n 2.-pri \n ");
	printf("\n 3.-pan \n ");
	printf("\n 4.-prd \n ");
	printf("\n *********************************************** \n ");
	scanf("%d",&cand);
	switch (cand)
	{
	case 1:
		morena ++;
		break;
			
	case 2:
		pri ++;
		break;
		
	case 3:
		pan ++;
		break;
	case 4:
		prd ++;
		break;
	default:
		printf("Error de Ingreso de datos:");
			
	}
	printf("\n Tu resultado es: %d %d %d %d ",morena,pri,pan,prd);
	
}
     
      
	getch;
}

